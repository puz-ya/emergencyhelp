<resources>

    <!-- APP NAME must be 10 letters MAX -->
    <string name="app_name">Safe Trip</string>

    <!-- SMS MESSAGE -->
    <string name="message_template">
        Heavy hit registered ($time). It may be $name_sentence need help.
        $addition_info_sentence $location_sentence
        This is an automatic sms.
    </string>
    <string name="name_sentence">I: $name</string>
    <string name="addition_info_sentence">Info: $addition_info.</string>
    <string name="location_sentence">Coord. ($minutes_pass min. ago): lat.=$latitude, long.=$longitude, $accuracy_sentence.</string>
    <string name="accuracy_sentence">(accur. $accuracy m.)</string>
    <string name="location_error_sentence">Location: unknown.</string>


