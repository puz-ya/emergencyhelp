    <string name="gps_off_dialog_positive_button">OK</string>
    <string name="gps_off_dialog_negative_button">Cancel</string>

    <!-- OPEN SETTINGS ON FIRST LAUNCH DIALOG  -->
    <string name="first_launch_open_settings_dialog_description">
